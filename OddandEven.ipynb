    "def number(n):\n",
    "        if n % 4 ==0:\n",
    "            print(str(n) + \" is a multiple of 4.\")\n",
    "        elif n % 2 == 0:\n",
    "            print(str(n) + \" is an Even number.\")\n",
    "        elif n % 2 != 0:\n",
    "            print(str(n) + \" is an Odd number.\")\n",
    "number(20)"
   ]
